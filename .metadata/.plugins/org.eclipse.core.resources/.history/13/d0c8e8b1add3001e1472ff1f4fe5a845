package com.rowdy.dino.graphics;

import java.awt.Graphics;

import com.rowdy.dino.Main;

public class Scene {
	private boolean showing = false;
	public Scene() {
		
	}
	
	public boolean isShowing() {
		return showing;
	}

	public boolean setShowing(boolean showing) {
		this.showing = showing;
		return showing;
	}
	
	public void render(Graphics g) {
		g.drawRect(Main.navWidth, 0, Main.frameSize.width - Main.navWidth, Main.frameSize.height);
	}
	
	public void tick() {
		
	}
}
